APP_PORT=8000
HOST=0.0.0.0
# LiveKit Configuration
LIVEKIT_URL=wss://your-livekit-server.com
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# OpenRouter Configuration (REQUIRED)
OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxxxxxxxxxx
LLM_MODEL=openai/gpt-4o-mini
# Other available models:
# - openai/gpt-4o
# - anthropic/claude-3.5-sonnet
# - google/gemini-pro
# - meta-llama/llama-3.1-70b-instruct
LLM_TEMPERATURE=0.7

# Speech-to-Text Configuration
STT_PROVIDER=whisper
STT_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxx  # OpenAI API key for Whisper
STT_MODEL=whisper-1
STT_LANGUAGE=en

# Text-to-Speech Configuration
TTS_PROVIDER=openai
TTS_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxx  # OpenAI API key
TTS_VOICE_ID=nova
# Available voices: alloy, echo, fable, onyx, nova, shimmer
TTS_MODEL=tts-1

# Vector Database Configuration
VECTOR_DB_PROVIDER=faiss
VECTOR_DB_API_KEY=  # Leave empty for FAISS (local)
VECTOR_DB_INDEX=ecommerce-products

# Database Configuration
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/ecommerce

# Redis Configuration
REDIS_URL=redis://localhost:6379